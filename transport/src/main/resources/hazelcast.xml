<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:hz="http://www.hazelcast.com/schema/spring"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
                http://www.springframework.org/schema/beans/spring-beans.xsd
                http://www.hazelcast.com/schema/spring
                http://www.hazelcast.com/schema/spring/hazelcast-spring-2.5.xsd">

    <hz:hazelcast id="instance">
        <hz:config>
            <hz:group name="${hz.config.group.name}" password="${hz.config.group.password}"/>

            <hz:management-center enabled="${hz.config.management-center.enabled}"
                                  url="${hz.config.management-center.url}"/>

            <hz:network port-auto-increment="${hz.config.network.port-auto-increment}" port="${hz.config.network.port}">

                <hz:outbound-ports>
                    <hz:ports>${hz.config.network.outbound-ports.ports}</hz:ports>
                </hz:outbound-ports>

                <hz:join>

                    <hz:multicast enabled="${hz.config.network.join.multicast.enabled}"
                                  multicast-group="${hz.config.network.join.multicast.multicast-group}"
                                  multicast-port="${hz.config.network.join.multicast.multicast-port}"/>

                    <hz:tcp-ip enabled="${hz.config.network.join.tcp-ip.enabled}">
                        <hz:interface>${hz.config.network.join.tcp-ip.interface}</hz:interface>
                    </hz:tcp-ip>

                    <hz:aws enabled="${hz.config.network.join.aws.enabled}"
                            access-key="${hz.config.network.join.aws.access-key}"
                            secret-key="${hz.config.network.join.aws.secret-key}"
                            region="${hz.config.network.join.aws.region}"
                            host-header="${hz.config.network.join.aws.host-header}"
                            security-group-name="${hz.config.network.join.aws.security-group-name}"
                            tag-key="${hz.config.network.join.aws.tag-key}"
                            tag-value="${hz.config.network.join.aws.tag-value}"/>

                </hz:join>

                <hz:interfaces enabled="${hz.config.interfaces.enabled}">
                    <hz:interface>${hz.config.interfaces.interface}</hz:interface>
                </hz:interfaces>

                <hz:ssl enabled="${hz.config.ssl.enabled}"/>

                <hz:socket-interceptor enabled="${hz.config.socket-interceptor.enabled}"/>

                <hz:symmetric-encryption enabled="${hz.config.symmetric-encryption.enabled}"
                                         algorithm="${hz.config.symmetric-encryption.algorithm}"
                                         salt="${hz.config.symmetric-encryption.salt}"
                                         password="${hz.config.symmetric-encryption.password}"
                                         iteration-count="${hz.config.symmetric-encryption.iteration-count}"/>

                <hz:asymmetric-encryption enabled="${hz.config.asymmetric-encryption.enabled}"
                                          algorithm="${hz.config.asymmetric-encryption.algorithm}"
                                          key-password="${hz.config.asymmetric-encryption.key-password}"
                                          key-alias="${hz.config.asymmetric-encryption.key-alias}"
                                          store-type="${hz.config.asymmetric-encryption.store-type}"
                                          store-password="${hz.config.asymmetric-encryption.store-password}"
                                          store-path="${hz.config.asymmetric-encryption.store-path}"/>
            </hz:network>

            <hz:partition-group enabled="false"/>

            <hz:executor-service name="executor-service" core-pool-size="${hz.config.executor-service.core-pool-size}"
                                 max-pool-size="${hz.config.executor-service.max-pool-size}"
                                 keep-alive-seconds="${hz.config.executor-service.keep-alive-seconds}"/>

            <hz:queue name="${hz.config.queue.name}" max-size-per-jvm="${hz.config.queue.max-size-per-jvm}"/>

            <hz:map name="default" backup-count="${hz.config.map.backup-count}"
                    async-backup-count="${hz.config.map.async-backup-count}"
                    time-to-live-seconds="${hz.config.map.time-to-live-seconds}"
                    max-idle-seconds="${hz.config.map.max-idle-seconds}"
                    eviction-policy="${hz.config.map.eviction-policy}"
                    max-size-policy="${hz.config.map.max-size-policy}" max-size="${hz.config.map.max-size}"
                    eviction-percentage="${hz.config.map.eviction-percentage}"
                    merge-policy="${hz.config.map.merge-policy}"/>

        </hz:config>
    </hz:hazelcast>
</beans>