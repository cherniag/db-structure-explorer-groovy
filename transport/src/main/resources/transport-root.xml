       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-3.0.xsd http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">

    <import resource="classpath:META-INF/service.xml"/>
    <import resource="classpath:META-INF/dao.xml"/>
    <import resource="classpath:META-INF/shared.xml"/>
    <import resource="classpath:META-INF/soap.xml"/>
    <import resource="classpath:META-INF/smpp.xml"/>
