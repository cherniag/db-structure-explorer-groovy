<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean name="communityServiceFactory" class="mobi.nowtechnologies.server.web.model.CommunityServiceFactory" />

    <bean id="mtvnz.PinModelService" class="mobi.nowtechnologies.server.web.model.mtvnz.PinPinModelServiceImpl">
        <property name="paymentPolicyService" ref="service.PaymentPolicyService"/>
    </bean>

    <bean id="mtvnz.PaymentModelService" class="mobi.nowtechnologies.server.web.model.mtvnz.PaymentModelServiceImpl">
        <property name="paymentPolicyService" ref="service.PaymentPolicyService"/>
    </bean>

    <alias name="service.vfPaymentService" alias="mtvnz.PSMSPaymentService" />

</beans>
