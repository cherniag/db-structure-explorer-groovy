<?xml version="1.0" encoding="UTF-8"?>
<beans default-lazy-init="true" xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans.xsd">   

    <bean name="trackRepo.SignInController" class="mobi.nowtechnologies.server.trackrepo.controller.SignInController"/>
    
     <bean name="trackRepo.FileController"
		class="mobi.nowtechnologies.server.trackrepo.controller.FileController">
		<property name="fileService" ref="trackRepo.FileService" />
	</bean>
    
    <bean name="trackRepo.TrackController"
		class="mobi.nowtechnologies.server.trackrepo.controller.TrackController">
		<property name="trackService" ref="trackRepo.TrackService" />
		<property name="executorTimeout" value="${trackRepo.executor.timeout}" />
		<property name="resourceFileDtoBuilder" ref="trackRepo.ResourceFileDtoBuilder" />
	</bean>

    <bean name="trackRepo.IngestTracksWizardController"
		class="mobi.nowtechnologies.server.trackrepo.controller.IngestTracksWizardController">
		<property name="ingestService" ref="trackRepo.IngestService" />
	</bean>

	<bean name="trackRepo.TrackReportingOptionsController" class="mobi.nowtechnologies.server.trackrepo.controller.TrackReportingOptionsController">
		<property name="trackReportingOptionsService" ref="trackReportingOptionsService"/>
 	</bean>

	<bean class="mobi.nowtechnologies.server.support.props.PropController">
		<property name="propertyPlaceholderConfigurer" ref="propertyPlaceholderConfigurer"/>
 	</bean>
</beans>